{
  "github": {
    "enabled": true,
    "autoAlias": false,
    "autoJobCancelation": true,
    "checks": {
      "ignoreFailures": false
    }
  },
  "buildCommand": "npm run build",
  "installCommand": "npm ci",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Link",
          "value": "</_next/static/css/>; rel=preload; as=style"
        }
      ]
    }
  ]
}
